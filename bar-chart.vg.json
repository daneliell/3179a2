{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 300,
    "height": 200,
    "background": "#f9f7f1",
    "data": {
        "url": "https://raw.githubusercontent.com/daneliell/3179a2/main/msia_assets.csv"
    },
    "transform": [{
        "filter": "datum.Asset != 'Total Assets'"
    }],
    "mark": {"type": "line"},
    "encoding": {
      "x": {
        "field": "Year", 
        "type": "temporal",
        "axis": {"grid": false}
        },
      "y": {
        "field": "Value", 
        "type": "quantitative",
        "axis": {"title": "Value (RM)", "grid": false}
        },
      "color": {
          "field": "Asset",
          "type": "nominal"
        },
        "tooltip": [
        {"field": "Year", "type": "temporal"},
        {"field": "Value", "type": "quantitative", "format": "0.2f"},
        {"field": "Asset", "type": "nominal"}
        ]
    }
  }
  